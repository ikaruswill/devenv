"*":
  "advanced-open-file":
    createFileInstantly: true
  "atom-clock": {}
  "atom-terminal":
    app: "iTerm.app"
    surpressDirectoryArgument: false
  "autocomplete-python":
    extraPaths: "$PROJECT/venv/python2.7/site-packages;$PROJECT/venv/python3.6/site-packages"
    pythonPaths: "$PROJECT/venv/bin/python"
    useKite: false
    useSnippets: "required"
  "bracket-matcher": {}
  "city-lights-ui": {}
  core:
    disabledPackages: [
      "minimap-autohide"
      "linter-pycodestyle"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "*.pyc"
      "venv"
      "env"
    ]
    packagesWithKeymapsDisabled: [
      "zentabs"
    ]
    projectHome: "~/Dev"
    telemetryConsent: "limited"
    themes: [
      "seti-ui"
      "flatland-dark"
    ]
    titleBar: "custom"
  editor:
    fontFamily: "Inconsolata"
    scrollSensitivity: 60
    tabLength: 4
  "exception-reporting":
    userId: "ff279c35-0461-40b8-dbbb-67ed8b54de5d"
  "find-and-replace":
    scrollToResultOnLiveSearch: true
  linter:
    disabledProviders: [
      "Pylint"
    ]
  "linter-pycodestyle":
    executablePath: "venv/bin/pycodestyle"
  "linter-pylint":
    executablePath: "%p/venv/bin/pylint"
    pythonPath: "%p/venv/bin/python"
  "linter-ui-default":
    panelHeight: 56
    statusBarRepresents: "Current File"
  minimap:
    disablePythonDocLinks: true
    plugins:
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      linter: false
      linterDecorationsZIndex: 0
      "minimap-autohide": true
      "minimap-autohideDecorationsZIndex": 0
      selection: true
      selectionDecorationsZIndex: 0
  "project-manager":
    includeGitRepositories: true
    savePathsRelativeToHome: true
    sortBy: "last modified"
  "python-isort":
    isortPath: "/usr/local/bin/isort"
  "seti-ui":
    compactView: true
    hideProjectTab: true
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    scrollSyncType: "Vertical + Horizontal"
  "status-bar": {}
  tabs: {}
  "todo-show":
    sortAscending: false
    sortBy: "Type"
  "tree-view":
    autoReveal: true
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
  whitespace: {}
  zentabs:
    maximumOpenedTabs: 6
    neverCloseDirty: true
    neverCloseNew: true
    neverCloseUnsaved: true
".source.yaml":
  editor:
    tabLength: 2
